<header>
  <h1>
    <%= link_to "Archives", archives_path %>
    <time><%= link_to_dates(year: @articles_year, month: @articles_month, day: @articles_day) %></time>
  </h1>
</header>

<div class="h-feed">

  <% @articles.each do |article| %>
    <%= render "/articles/tile", article: article %>
  <% end %>

</div>
